{
  "version": 2,
  "builds": [
    {
      "src": "serverless.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "serverless.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}