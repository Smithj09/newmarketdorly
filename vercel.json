{
  "version": 2,
  "build": {
    "command": "npm run build",
    "outputDirectory": "dist"
  },
  "builds": [
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "server.cjs"
    }
  ]
}