{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "serverless.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "serverless.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}